{
  "name": "nextn",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "dev": "npm run db:push && npm run db:init && tsx --env-file=.env node_modules/.bin/next dev --port 9002 --hostname 0.0.0.0",
    "build": "rm -rf node_modules package-lock.json && npm install && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:init": "tsx --env-file=.env scripts/init-db.ts",
    "db:seed": "tsx --env-file=.env scripts/seed-db.ts",
    "db:push": "npx prisma db push --accept-data-loss",
    "postinstall": "npx prisma generate",
    "test": "NODE_ENV=test tsx --env-file=.env tests/bidding-e2e.test.ts",
    "test:ui": "npx playwright test",
    "test:e2e": "npm run build && npm run test:ui"
  },
