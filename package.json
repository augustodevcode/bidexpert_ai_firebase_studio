{
    "name": "bid-expert",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npx prisma db push --accept-data-loss && next dev --port 9002 --hostname 0.0.0.0",
        "build": "prisma generate && next build",
        "start": "next start",
        "postinstall": "prisma generate",
        "lint": "next lint",
        "db:init": "tsx --env-file=.env scripts/init-db.ts",
        "db:seed": "tsx --env-file=.env scripts/seed-db.ts",
        "db:seed:samples": "tsx --env-file=.env scripts/seed-db-sample-data.ts",
        "db:seed:extended": "tsx --env-file=.env scripts/seed-data-extended.ts",
        "db:migrate": "prisma migrate dev",
        "db:push": "prisma db push --accept-data-loss",
        "db:reset": "npm run db:init && npm run db:seed",
        "test": "vitest",
        "test:e2e": "vitest run tests/bidding-e2e.test.ts",
        "test:ui": "playwright test",
        "test:ui:headed": "playwright test --headed",
        "test:ui:debug": "playwright test --debug",
        "test:ui:report": "playwright show-report",
        "test:ui:clean": "rm -rf test-results playwright-report",
        "clean": "rm -rf .next dist build out && rm -rf tests/ui/test-results && rm -rf tests/ui/playwright-report"
    },
