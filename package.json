{
    "name": "bid-expert",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npx prisma db push --accept-data-loss && npx next dev --port 9002 --hostname 0.0.0.0",
        "build": "npx prisma generate && next build",
        "start": "next start",
        "postinstall": "npx prisma generate",
        "lint": "next lint",
        "db:init": "npx tsx --env-file=.env scripts/init-db.ts",
        "db:seed": "npx tsx --env-file=.env scripts/seed-db.ts",
        "db:seed:samples": "npx tsx --env-file=.env scripts/seed-db-sample-data.ts",
        "db:seed:extended": "npx tsx --env-file=.env scripts/seed-data-extended.ts",
        "db:migrate": "npx prisma migrate dev",
        "db:push": "npx prisma db push --accept-data-loss",
        "db:reset": "npm run db:init && npm run db:seed",
        "test": "vitest",
        "test:e2e": "vitest run tests/bidding-e2e.test.ts",
        "test:ui": "playwright test",
        "test:ui:headed": "playwright test --headed",
        "test:ui:debug": "playwright test --debug",
        "test:ui:report": "playwright show-report",
        "test:ui:clean": "rm -rf test-results playwright-report",
        "clean": "rm -rf .next dist build out && rm -rf tests/ui/test-results && rm -rf tests/ui/playwright-report"
    },
