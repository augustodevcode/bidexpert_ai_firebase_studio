{
    "name": "bid-expert",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npx prisma generate && next dev --port 9002 --hostname 0.0.0.0",
        "dev:with-seed": "npx prisma db push && npx tsx --env-file=.env scripts/seed-db.ts && npx next dev --port 9002 --hostname 0.0.0.0",
        "build": "npx prisma generate && next build",
        "build:local": "prisma generate && next build",
        "start": "next start",
        "postinstall": "npx prisma generate",
        "lint": "next lint",
        "db:init": "npx tsx --env-file=.env scripts/init-db.ts",
        "db:seed": "npx tsx --env-file=.env scripts/seed-data-extended.ts",
        "db:seed:samples": "npx tsx --env-file=.env scripts/seed-db-sample-data.ts",
        "db:seed:extended": "npx tsx --env-file=.env scripts/seed-data-extended.ts",
        "db:seed:resume": "npx tsx --env-file=.env scripts/seed-resume.ts",
        "db:seed:auctions": "npx tsx --env-file=.env scripts/seed-auctions-only.ts",
        "db:check-status": "npx tsx --env-file=.env scripts/check-seed-status.ts",
        "db:migrate": "npx prisma migrate dev",
        "db:push": "npx prisma db push",
        "db:reset": "npm run db:init && npm run db:seed",
        "clean": "rm -rf .next dist build out && rm -rf tests/ui/test-results && rm -rf tests/ui/playwright-report"
    },
