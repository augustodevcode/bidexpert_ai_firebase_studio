{
    "name": "bid-expert",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm run prisma:build && npx prisma db push && npm run db:seed && next dev --port 9003 --hostname 0.0.0.0",
        "start": "next start --port 9002 --hostname 0.0.0.0",
        "build": "npm run prisma:build && npx prisma generate && next build",
        "reinstall": "rm -rf node_modules package-lock.json .next dist && npm cache clean --force && npm install",
        "lint": "next lint",
        "db:seed": "tsx --env-file=.env scripts/seed-db.ts",
        "db:push": "npm run prisma:build && npx prisma db push --accept-data-loss",
        "prisma:build": "tsx scripts/build-prisma-schema.ts",
        "test": "vitest",
        "test:e2e": "vitest run tests/bidding-e2e.test.ts",
        "postinstall": "npm run prisma:build && npx prisma generate"
    },
