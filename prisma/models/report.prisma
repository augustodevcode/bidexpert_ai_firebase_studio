model Report {
  id          BigInt   @id @default(autoincrement())
  name        String
  description String?
  definition  Json
  
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  
  tenantId    BigInt
  tenant      Tenant @relation(fields: [tenantId], references: [id])
  
  ownerId     BigInt?
  owner       User?    @relation(fields: [ownerId], references: [id])
}
