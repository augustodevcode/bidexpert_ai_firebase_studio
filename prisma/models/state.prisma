// prisma/models/state.prisma
import { City, JudicialDistrict, Asset } from "./";

model State {
  id        BigInt     @id @default(autoincrement())
  name      String   @unique
  uf        String   @unique
  slug      String   @unique

  cities    City[]
  districts JudicialDistrict[]
  assets    Asset[]
}
