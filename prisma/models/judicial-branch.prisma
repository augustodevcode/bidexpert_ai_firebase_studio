model JudicialBranch {
  id          BigInt   @id @default(autoincrement())
  name        String
  slug        String   @unique
  contactName String?
  phone       String?
  email       String?

  districtId BigInt
  district   JudicialDistrict @relation(fields: [districtId], references: [id])

  judicialProcesses JudicialProcess[]
  auctions          Auction[]
  sellers           Seller[]

  @@unique([districtId, name])
}
