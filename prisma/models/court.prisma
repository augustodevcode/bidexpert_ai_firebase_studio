// prisma/models/court.prisma
model Court {
  id                Int       @id @default(autoincrement())
  name              String    @unique
  slug              String    @unique
  stateUf           String
  website           String?
  createdAt         DateTime  @default(now())
  updatedAt         DateTime  @updatedAt
  judicialDistricts JudicialDistrict[]
  judicialProcesses JudicialProcess[]
  auctions          Auction[]
}
