// prisma/models/users-on-roles.prisma
import { User, Role } from "./";

model UsersOnRoles {
  user       User     @relation(fields: [userId], references: [id])
  userId     String
  role       Role     @relation(fields: [roleId], references: [id])
  roleId     String
  assignedAt DateTime @default(now())
  assignedBy String
  
  @@id([userId, roleId])
}
