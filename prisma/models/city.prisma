model City {
  id        BigInt   @id @default(autoincrement())
  name      String
  slug      String?  @unique
  stateId   String
  state     State    @relation(fields: [stateId], references: [id])
  ibgeCode  String?  @unique
  auctions  Auction[]
  lots      Lot[]
  sellers   Seller[]
  assets    Asset[]

  @@unique([name, stateId])
}
