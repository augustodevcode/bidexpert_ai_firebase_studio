// prisma/models/city.prisma
import { State, Asset } from "./";

model City {
  id      BigInt    @id @default(autoincrement())
  name    String
  slug    String    @unique
  state   State     @relation(fields: [stateId], references: [id])
  stateId BigInt
  stateUf String?
  ibgeCode String?   @unique

  assets   Asset[]

  @@unique([name, stateId])
}
