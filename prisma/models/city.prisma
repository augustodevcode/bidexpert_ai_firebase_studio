
model City {
  id        BigInt    @id @default(autoincrement())
  slug      String    @unique
  name      String
  stateId   BigInt
  state     State     @relation(fields: [stateId], references: [id])
  ibgeCode  String?
  latitude  Decimal?
  longitude Decimal?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  auctions  Auction[]
  lots      Lot[]

  @@unique([name, stateId])
}
