// prisma/models/assets-on-lots.prisma
import { Lot, Asset } from "./";

model AssetsOnLots {
  lot      Lot      @relation(fields: [lotId], references: [id])
  lotId    BigInt
  asset    Asset    @relation(fields: [assetId], references: [id])
  assetId  BigInt
  assignedAt DateTime @default(now())

  @@id([lotId, assetId])
}
