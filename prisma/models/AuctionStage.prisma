model AuctionStage {
  id           String   @id @default(cuid())
  name         String
  startDate    DateTime @db.DateTime(0)
  endDate      DateTime @db.DateTime(0)
  initialPrice Decimal? @db.Decimal(15, 2)
  auctionId    String

  auction Auction @relation(fields: [auctionId], references: [id], onDelete: Cascade)

  @@index([auctionId])
}
