// prisma/models/assetsOnLots.prisma

model AssetsOnLots {
  lot      Lot    @relation(fields: [lotId], references: [id])
  lotId    String
  asset    Asset  @relation(fields: [assetId], references: [id])
  assetId  String
  assignedAt DateTime @default(now())
  assignedBy String?

  @@id([lotId, assetId])
}
