# This file contains example environment variables.
# Copy this file to .env.local and fill in your actual values.
# IMPORTANT: .env.local should be added to .gitignore and never be committed.

# --- Database ---
# The connection string for your database.
# Used by Prisma to connect to the database.
# Format for PostgreSQL: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
# Format for MySQL: mysql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="postgresql://user:password@localhost:5432/bidexpert_db"


# --- Firebase Project (Client-side) ---
# These are used by the client-side Firebase SDK for features like authentication.
# These values are safe to expose to the browser.
NEXT_PUBLIC_FIREBASE_API_KEY="YOUR_API_KEY_HERE"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="YOUR_PROJECT_ID.firebaseapp.com"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="YOUR_PROJECT_ID"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="YOUR_PROJECT_ID.appspot.com"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="YOUR_SENDER_ID"
NEXT_PUBLIC_FIREBASE_APP_ID="YOUR_APP_ID"
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID="YOUR_MEASUREMENT_ID"


# --- Genkit AI ---
# API Key for Google AI services (e.g., Gemini).
# This is a server-side only variable.
GOOGLE_API_KEY="YOUR_GOOGLE_AI_API_KEY"


# --- Datajud CNJ API ---
# API Key for accessing the public data from CNJ.
# This is a server-side only variable.
DATAJUD_API_KEY="YOUR_DATAJUD_API_KEY"
