
/// Modelo para salvar as definições de relatórios criados no Construtor de Relatórios.
model Report {
  id          BigInt   @id @default(autoincrement())
  name        BigInt
  description BigInt?
  definition  Json
  
  tenantId    BigInt
  tenant      Tenant   @relation(fields: [tenantId], references: [id])
  
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
