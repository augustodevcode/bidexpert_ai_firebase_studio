
/// Modelo para salvar as definições de relatórios criados no Construtor de Relatórios.
model Report {
  id          String   @id @default(cuid())
  name        String
  description String?
  definition  Json
  
  tenantId    String
  tenant      Tenant   @relation(fields: [tenantId], references: [id])
  
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
