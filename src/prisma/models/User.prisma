
model User {
  id                    BigInt                 @id @default(autoincrement())
  publicId              BigInt?                @unique @default(cuid())
  email                 BigInt                 @unique
  password              BigInt?
  fullName              BigInt?
  cpf                   BigInt?                @unique
  rgNumber              BigInt?
  rgIssuer              BigInt?
  rgState               BigInt?
  rgIssueDate           DateTime?
  dateOfBirth           DateTime?
  gender                BigInt?
  profession            BigInt?
  nationality           BigInt?
  maritalStatus         BigInt?
  propertyRegime        BigInt?
  spouseName            BigInt?
  spouseCpf             BigInt?
  cellPhone             BigInt?
  homePhone             BigInt?
  zipCode               BigInt?
  street                BigInt?
  number                BigInt?
  complement            BigInt?
  neighborhood          BigInt?
  city                  BigInt?
  state                 BigInt?
  avatarUrl             BigInt?
  dataAiHint            BigInt?
  optInMarketing        Boolean                @default(false)
  habilitationStatus    UserHabilitationStatus @default(PENDING_DOCUMENTS)
  accountType           AccountType            @default(PHYSICAL)
  razaoSocial           BigInt?
  cnpj                  BigInt?                @unique
  inscricaoEstadual     BigInt?
  website               BigInt?
  responsibleName       BigInt?
  responsibleCpf        BigInt?
  createdAt             DateTime               @default(now())
  updatedAt             DateTime               @updatedAt

  // Relationships
  roles                 UsersOnRoles[]
  tenants               UsersOnTenants[]
  bids                  Bid[]
  wins                  UserWin[]
  maxBids               UserLotMaxBid[]
  questions             LotQuestion[]
  reviews               Review[]
  documents             UserDocument[]
  uploadedMedia         MediaItem[]
  sellerProfile         Seller?
  auctioneerProfile     Auctioneer?
}
