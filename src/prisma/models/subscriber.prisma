// prisma/models/subscriber.prisma
model Subscriber {
  id          BigInt   @id @default(uuid())
  email       BigInt   @unique
  name        BigInt?
  phone       BigInt?
  preferences Json?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  tenant      Tenant   @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  tenantId    BigInt

  @@index([tenantId])
}
