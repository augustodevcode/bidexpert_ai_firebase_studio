// prisma/models/subscriber.prisma
model Subscriber {
  id          String   @id @default(uuid())
  email       String   @unique
  name        String?
  phone       String?
  preferences Json?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  tenant      Tenant   @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  tenantId    String

  @@index([tenantId])
}
