
// Este é o cabeçalho do schema do Prisma.
// Contém as configurações do gerador e da fonte de dados.
// Modelos de dados devem ser definidos em arquivos separados em /prisma/models.

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
